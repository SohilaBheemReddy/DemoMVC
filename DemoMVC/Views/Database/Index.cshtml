@model DemoMVC.Models.EMPDATA
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@using (Html.BeginForm())
{ 
<table>
    <tr>
        <td>
            Empno
        </td>
        <td>
            @Html.TextBoxFor(a => a.EMPNO)
        </td>
    </tr>
    <tr>
        <td>
            Employee Name
        </td>
        <td>
            @Html.TextBoxFor(a => a.ENAME)
        </td>
    </tr>
    <tr>
        <td>
            Job
        </td>
        <td>
            @Html.TextBoxFor(a => a.JOB)
        </td>
    </tr>
    <tr>
        <td>
            Manager ID
        </td>
        <td>
            @Html.TextBoxFor(a => a.MGR)
        </td>
    </tr>
    <tr>
        <td>
            HireDate
        </td>
        <td>
            @Html.TextBoxFor(a => a.HIREDATE)
        </td>
    </tr>

    <tr>
        <td>
            Salary
        </td>
        <td>
            @Html.TextBoxFor(a => a.SAL)
        </td>
    </tr>
    <tr>
        <td>
            Commission
        </td>
        <td>
            @Html.TextBoxFor(a => a.COMM)
        </td>
    </tr>
    <tr>
        <td>
            Deptno
        </td>
        <td>
            @Html.TextBoxFor(a => a.DEPTNO)
        </td>
    </tr>
    <tr>
        <td><input type="submit" value="Insert"/></td>
    </tr>
</table>
if (ViewBag.msg != null)
{
<h3>
    @ViewBag.msg
</h3>
 }


}

